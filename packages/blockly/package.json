{
  "name": "@turbomixer/editor-blockly",
  "version": "0.0.2-prealpha.1",
  "description": "The core module for turbomixer",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./client": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "client": ["./dist"]
    }
  },
  "files": [
    "package.json",
    "lib",
    "dist"
  ],
  "repository": "https://github.com/turbomixer/turbomixer",
  "author": "TimeBather<open-source@timebather.cn>",
  "license": "MIT",
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "blockly-multiverse": "^0.0.2-prealpha.1"
  },
  "peerDependencies": {
    "@turbomixer/core": "^0.0.2-prealpha.1"
  },
  "compiler": "plugin",
  "devDependencies": {
    "vite-plugin-dts": "^3.6.4"
  }
}
